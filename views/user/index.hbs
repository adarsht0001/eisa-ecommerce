  <!--================Home Banner Area =================-->
  {{#if first}}
  <section class="home_banner_area mb-40">
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" style="z-index:100;">
      <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        {{#each banner}}
        <li data-target="#carouselExampleIndicators" data-slide-to={{@index}}></li>
        {{/each}}
      </ol>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img class="d-block w-100" src="/product/{{first.imagefileName}}" alt="First slide" style="height: 45em;">
          <div class="carousel-caption d-none d-md-block pb-5">
            <p class="sub text-uppercase">{{first.Title}}</p>
            {{!-- <h3><span>Show</span> Your <br />Personal <span>Style</span></h3>
            <h4>Fowl saw dry which a above together place.</h4> --}}
            <a class="main_btn" href="/shop">View Collection</a>
          </div>
        </div>
        {{#each banner}}
        <div class="carousel-item">
          <img class="d-block w-100" src="/product/{{this.imagefileName}}" alt="Second slide" style="height: 45em;">
          <div class="carousel-caption d-none d-md-block">
            <div class="banner_content row">
              <div class="col-lg-12 pb-5">
                <h2 class="sub text-uppercase text-white">{{this.Title}}</h2>
                <a class="main_btn mt-40" href="/shop">View Collection</a>
              </div>
            </div>
            {{!-- <a class="main_btn" href={{this.redirect}}>Go Shopping</a> --}}
          </div>
        </div>
        {{/each}}
        {{!-- <div class="carousel-item">
          <img class="d-block w-100" src="/images/12.jpg" alt="Third slide">
        </div> --}}
      </div>
      <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </section>
  {{else}}
  <section class="home_banner_area mb-40">
    <div class="banner_inner d-flex align-items-center">
      <div class="container">
        <div class="banner_content row">
          <div class="col-lg-12">
